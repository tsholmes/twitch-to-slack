{
    "env": {
      "browser": true
    },
    "extends": ["eslint:recommended"],
    "rules": {
        "indent": ["error", 2],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "eqeqeq": ["error", "always"],
        "no-sequences": ["error"],
        "consistent-return": ["error"],
        "strict": ["error", "global"]
    }
}
